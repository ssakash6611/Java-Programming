public class mylab10
{
      public static void main(String[]args)
      {
        int[]a={7,13,2,10,6};
        int temp=0;
         for(int i=0;i<a.length;i++)
         {
               for( int j=0;j<a.length-1;J++)
               {
                    if(a[i]>a[i++])
                    {
                      temp=a[i];
                      a[i]=a[i++];
                      a[i++]=temp
                    }
               }
               System.out.println(a[i]);
         }
      }
}
               
               